var offX = -1000/2;
var offY = -500/2;

function onLoad(stage){
	var sky = new FlxSprite(offX, offY);
	sky.loadGraphic(Paths.image("extra1/extra1 sky"));	
	sky.scrollFactor.set(0.3, 0);
	stage.add(sky);

	var ground = new FlxSprite(offX, offY + 756);
	ground.loadGraphic(Paths.image("extra1/extra1 ground"));	
	ground.scrollFactor.set(1, 0.95);
	stage.add(ground);
}